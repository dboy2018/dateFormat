<script>
    Vue.filter("dateFormat", function (value, formatStr) {
        console.log(typeof value);
        return value.format(formatStr);
    });
    let vm = new Vue({
        el: "#app",
        data: {
            date: new Date()
        },
        methods: {}
    });
</script>
